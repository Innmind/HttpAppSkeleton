{
    "name": "innmind/http-app-skeleton",
    "type": "project",
    "description": "http app skeleto,",
    "keywords": ["http", "app"],
    "homepage": "http://github.com/Innmind/HttpAppSkeleton",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "langlade.baptiste@gmail.com"
        }
    ],
    "support": {
        "issues": "http://github.com/Innmind/HttpAppSkeleton/issues"
    },
    "require": {
        "php": "~7.2",
        "innmind/http-server": "^1.0",
        "innmind/http-framework": "^1.0",
        "innmind/templating": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "AppName\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\AppName\\": "tests/",
            "Fixture\\AppName\\": "fixtures/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "giorgiosironi/eris": "^0.10.0",
        "filp/whoops": "^2.2",
        "mnapoli/silly": "^1.7",
        "innmind/server-control": "^2.5",
        "innmind/graphviz": "^1.1"
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always"
    }
}
